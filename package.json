{
  "name": "gbbo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "setup": "turbo run setup",
    "turbo": "turbo",
    "postinstall": "cd packages/api && poetry install && cd - >/dev/null 2>&1",
    "scraper:build": "npm -w @gbbo/scraper run build",
    "scraper:dev": "npm -w @gbbo/scraper run dev",
    "scraper:scrape": "npm -w @gbbo/scraper run scrape",
    "scraper:setup": "npm -w @gbbo/scraper run setup"
  },
  "devDependencies": {
    "turbo": "^2.5.8"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "packageManager": "npm@11.6.1"
}
